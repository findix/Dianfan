<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.devMode" value="true" />

	<package name="dianfan" namespace="/" extends="struts-default">
		<default-action-ref name="index" />
		<action name="index">
			<result>
				index.jsp
			</result>
		</action>
		<action name="login">
			<result>
				login.jsp
			</result>
		</action>
		<action name="SignUpAction" class="com.find1x.dianfan.action.SignUpAction">
		    <result>
				result.jsp
			</result>
		</action>
		<action name="LoginAction" class="com.find1x.dianfan.action.LoginAction">
		    <result>
				result.jsp
			</result>
		</action>
	</package>
</struts>
